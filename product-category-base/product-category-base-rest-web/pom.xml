<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.gdn.x.productcategorybase</groupId>
        <artifactId>product-category-base-parent</artifactId>
        <version>6.16.0-3-SNAPSHOT</version>
    </parent>
    <artifactId>product-category-base-rest-web</artifactId>
    <name>${project.groupId}-${project.artifactId}</name>
    <properties>
        <jar.name>product-category-base</jar.name>
        <sonar.exclusions>
            **/csv/**,**/dto/**,**/entity/**,**/mixin/**,**/model/**,**/config/**,**/ProductCategoryBaseApplication*.java
        </sonar.exclusions>
        <maven.compiler.source>21</maven.compiler.source>
        <maven.compiler.target>21</maven.compiler.target>
    </properties>
    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <configuration>
                    <executable>false</executable>
                    <mainClass>com.gdn.x.productcategorybase.ProductCategoryBaseApplication
                    </mainClass>
                </configuration>
            </plugin>
        </plugins>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <filtering>true</filtering>
                <includes>
                    <include>**/*.properties</include>
                    <include>**/*.xml</include>
                </includes>
            </resource>
        </resources>
    </build>

    <dependencies>
        <dependency>
            <groupId>com.gdn.x.productcategorybase</groupId>
            <artifactId>product-category-base-service-api</artifactId>
        </dependency>
        <dependency>
            <groupId>com.gdn.x.productcategorybase</groupId>
            <artifactId>product-category-base-dao-jpa</artifactId>
        </dependency>
        <dependency>
            <groupId>com.gdn.x.productcategorybase</groupId>
            <artifactId>product-category-base-util</artifactId>
        </dependency>
        <dependency>
            <groupId>com.gdn.x.productcategorybase</groupId>
            <artifactId>product-category-base-service-impl</artifactId>
        </dependency>
        <dependency>
            <groupId>com.gdn.x.productcategorybase</groupId>
            <artifactId>product-category-base-index-job</artifactId>
        </dependency>
        <dependency>
            <groupId>com.gdn.x.productcategorybase</groupId>
            <artifactId>product-category-base-domain-event</artifactId>
        </dependency>
        <dependency>
            <groupId>com.gdn.x.productcategorybase</groupId>
            <artifactId>product-category-base-dao-api</artifactId>
        </dependency>
    </dependencies>
</project>