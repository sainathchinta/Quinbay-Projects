    <artifactId>product-rest-web</artifactId>
    <name>${project.groupId}-${project.artifactId}</name>
    <properties>
        <sonar.exclusions>
            **/dto/**,**/entity/**,**/model/**,src/main/webapp/docs/lib/swagger.js,**/config/ExecutorConfigVirtualThread.java,
            **/config/ExecutorConfigPlatformThread.java,**/config/BeanConfig.java,**/config/KafkaConfig.java, **/config/KafkaProducerConfig.java,
            **/config/MongoConfig.java,**/config/PBPClientConfiguration.java,
            **/config/PersistenceConfig.java,**/config/PromotionClientConfiguration.java,
            **/config/RedisConfiguration.java,**/config/SolrConfig.java,
            **/config/SwaggerConfiguration.java,**/config/WebConfiguration.java,**/helper/api/TraceHelper.java,**/helper/TracerHelperImpl.java
            **/interceptor/AuditorAwareImpl.java,
            com/gdn/x/product/rest/web/helper/impl/CustomOperationCustomizer.java,
            **/interceptor/MandatoryParameterHelper.java,
            **/interceptor/MandatoryParameterInterceptor.java,
            **/interceptor/TimestampInterceptor.java,
            **/interceptor/WebAppInitializer.java,
            **/interceptor/KafkaConsumerInterceptor.java,
            **/properties/MandatoryParameter.java,
            **/properties/MandatoryParameterDefaultProperties.java,
            **/properties/RedisProperties.java,
            **/properties/SolrProperties.java,src/main/java/com/gdn/x/product/rest/web/ProductApplication.java
            src/main/java/com/gdn/x/product/rest/web/ErrorController.java,
            **/PrimaryMongoConfiguration.java,**/config/KafkaAutoStartListener.java
        </sonar.exclusions>
    </properties>
    <build>
        <finalName>${project.name}</finalName>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <configuration>
                    <executable>true</executable>
                    <mainClass>com.gdn.x.product.rest.web.ProductApplication</mainClass>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>3.0.0-M3</version>
                <configuration>
                    <rerunFailingTestsCount>3</rerunFailingTestsCount>
                    <reuseForks>true</reuseForks>
                    <useUnlimitedThreads>true</useUnlimitedThreads>
                    <forkCount>2.5C</forkCount>
                </configuration>
            </plugin>
        </plugins>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <filtering>true</filtering>
            </resource>
        </resources>
    </build>
